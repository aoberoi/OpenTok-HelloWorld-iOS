platform :ios, '8.0'

target 'HelloWorld' do
    # do not need to update this value to newer SDKs until the code leverages a feature that
    # is only present/compatible with a newer release. think of this as a "minimum". it translates
    # to '>= 2.6.0 && <= 3.0.0'
    pod 'OpenTok', '~> 2.6'
end
